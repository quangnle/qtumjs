{
  "name": "qtumjs",
  "version": "1.3.2",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build:commonjs": "tsc",
    "build:commonjs.d.ts": "tsc --declaration",
    "build:umd": "webpack --config webpack.config.js",
    "build": "npm run build:commonjs && npm run build:commonjs.d.ts && npm run build:umd",
    "postinstall": "yarn build"
  },
  "npmName": "qtumjs",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "dependencies": {
    "@types/isomorphic-fetch": "^0.0.34",
    "btoa": "^1.1.2",
    "ethjs-abi": "^0.2.1",
    "isomorphic-fetch": "^2.2.1",
    "url-parse": "^1.1.9"
  },
  "devDependencies": {
    "@types/node": "8.0.31",
    "ts-loader": "^2.3.7",
    "tslint": "^5.7.0",
    "typescript": "2.5.2",
    "webpack": "^3.6.0"
  }
}
